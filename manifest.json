{
    "manifest_version": 3,
    "name": "LinkedIn Personalized Connection Automator",
    "version": "1.0",
    "permissions": [
      "activeTab",
      "storage",
      "scripting"
    ],
    "content_scripts": [
      {
        "matches": ["https://www.linkedin.com/*"],
        "js": ["contentScript.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html"
    }
  }
  